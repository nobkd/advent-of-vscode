{
  "name": "advent-of-code-vscode",
  "displayName": "Advent of Code",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:AoC"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "aoc",
          "title": "Advent of Code",
          "icon": "res/aoc.svg"
        }
      ]
    },
    "views": {
      "aoc": [
        {
          "id": "description",
          "name": "Description",
          "icon": "res/aoc.svg",
          "contextualTitle": "View Description of AoC"
        },
        {
          "id": "data",
          "name": "Data",
          "icon": "res/aoc.svg",
          "contextualTitle": "Get Data for AoC Day",
          "visibility": "hidden"
        },
        {
          "id": "login",
          "name": "Login/Logout",
          "icon": "res/aoc.svg",
          "contextualTitle": "Log in/out from AoC",
          "visibility": "collapsed"
        }

      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  }
}
